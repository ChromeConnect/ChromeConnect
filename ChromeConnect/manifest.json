{
  "name": "Chrome Connect",
  "description": "A tool to connect through discussion.",
  "version": "0.1.0",
  "manifest_version": 3,
  "icons": {
    "16": "./images/get_started16.png",
    "32": "./images/get_started32.png",
    "48": "./images/get_started48.png",
    "128": "./images/get_started128.png"
  },
  "background": {
    "service_worker": "./background.js"
  },
  "action": {
    "default_popup": "./popup.html",
    "default_icons": {
      "16": "./images/get_started16.png",
      "32": "./images/get_started32.png",
      "48": "./images/get_started48.png",
      "128": "./images/get_started128.png"
    }
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "scripting",
    "identity",
    "identity.email"
  ],
  "host_permissions": ["https://sequelize.org/*", "http://localhost:8080/"]
}
